[core]
	editor = code --wait
	excludesfile = ~/rdkr/terminal/config/git/gitignore
	askpass = /usr/bin/ksshaskpass
[diff]
    tool = meld
[difftool]
    prompt = false
[difftool "meld"]
    cmd = meld "$LOCAL" "$REMOTE"
[rebase]
  abbreviateCommands
[pull]
	rebase = true
[init]
	defaultBranch = main
[difftool]
	prompt = false
[includeIf "gitdir:~/five/"]
	path = ~/rdkr/terminal/config/git/config-five
